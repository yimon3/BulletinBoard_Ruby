wb = xlsx_package.workbook
  wb.add_worksheet(name: "Posts") do |sheet|
    sheet.add_row %w(id post_title post_description post_status created_user_id updated_user_id deleted_user_id created_at updated_at deleted_at)
    @posts.each do |post|
      sheet.add_row [post.id, post.title, post.description, post.status, post.create_user_id, post.updated_user_id, post.deleted_user_id, post.created_at, post.updated_at, post.deleted_at]
    end
  end